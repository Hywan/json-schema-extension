{
    "name": "atoum/json-schema-extension",
    "type": "library",
    "description": "JSON Schema extension for atoum, the simple modern and intuitive unit testing framework for PHP 5.3+",
    "keywords": ["TDD","atoum","test","unit testing","json"],
    "homepage": "http://www.atoum.org",
    "license": "BSD",
    "authors": [
        {
            "name": "jubianchi",
            "email": "contact@jubianchi.fr"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jubianchi/atoum"
        }
    ],
    "require": {
        "justinrainbow/json-schema": "*"
    },
    "require-dev": {
        "atoum/atoum": "dev-experimentation",
        "atoum/praspel-extension": "*@dev",
            "hoa/core": "*@dev",
            "hoa/praspel": "*@dev",
            "hoa/console": "*@dev",
            "hoa/dispatcher": "*@dev",
            "hoa/math": "*@dev",
            "hoa/compiler": "*@dev",
            "hoa/string": "*@dev",
            "hoa/realdom": "*@dev",
            "hoa/file": "*@dev",
            "hoa/stream": "*@dev",
            "hoa/iterator": "*@dev",
            "hoa/visitor": "*@dev",
            "hoa/regex": "*@dev",
            "hoa/router": "*@dev"
    },
    "autoload": {
        "psr-4": {
            "mageekguy\\atoum\\faker\\": "classes"
        },
        "files": ["autoloader.php"]
    }
}
